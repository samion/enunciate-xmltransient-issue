<enunciate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="https://enunciate.webcohesion.com/schemas/enunciate-2.1.0.xsd">

    <title>Example</title>
    
    <api-classes>
        <include pattern="com.example.test.**" />
        <exclude pattern="**" />
    </api-classes>
    
    <namespaces>
        <namespace id="test" uri="http://example.com/test" />
    </namespaces>
    
    <modules>
        <docs/>
        <idl disableWadl="false">
            <schema namespace="http://example.com/test" filename="test.xsd" location="http://example.com/test/test.xsd" />
        </idl>
        <swagger disabled="true" />
        <c-xml-client disabled="true" />
        <csharp-xml-client disabled="true" disableCompile="true" />
        <jackson disabled="true" />
        <jackson1 disabled="true"  />
        <java-xml-client disabled="true" disableCompile="true" />
        <java-json-client disabled="true" disableCompile="true" />
        <jaxrs datatype-detection="local" groupBy="path" />
        <jaxb datatype-detection="local" />
        <jaxws disabled="true" />
        <gwt-json-overlay disabled="true" />
        <obj-c-xml-client disabled="true" />
        <php-xml-client disabled="true" />
        <php-json-client disabled="true" />
        <ruby-json-client disabled="true" />
    </modules>
</enunciate>
